<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <FrameLayout
        android:id="@+id/camera_preview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

    </FrameLayout>

    <ImageButton
        android:id="@+id/button_home"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@color/transparent"
        android:src="@drawable/home150"
        android:layout_gravity="right|bottom"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="20dp" />

    <ImageButton
        android:id="@+id/button_youtube"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@color/transparent"
        android:src="@drawable/youtube110"
        android:layout_gravity="right|center_vertical"
        android:layout_above="@+id/button_home"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:id="@+id/button_selfie"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@color/transparent"
        android:src="@drawable/selfie110"
        android:layout_gravity="right|bottom"
        android:layout_alignBottom="@+id/button_home"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="85dp"
        android:layout_marginRight="85dp" />

    <ImageButton
        android:id="@+id/button_facebook"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@color/transparent"
        android:src="@drawable/facebook110"
        android:layout_gravity="right|bottom"
        android:layout_alignBottom="@+id/button_home"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="5dp"
        android:layout_alignParentEnd="false"
        android:layout_marginRight="120dp" />

